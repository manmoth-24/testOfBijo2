<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/css/main.css">
    <link rel="stylesheet" href="/css/find.css">
</head>
<body onload="LoadTopBar('ランキング')">
    <!--トップバー、メイン意外入れる予定-->
    <div  id="topBar">

    </div>
    <h1>ランキング👑</h1>

    <table>
        <tr><th width="10%" class="num">番号</th><th>名前</th><th>票数</th></tr>
        <% var num = 0; %>
        <% for (var i = 0;i < datas.length; i++) { %>
            <%- include('./forinstance/votingTemp.ejs',
                {
                    num: ++num,
                    name: datas[i].name,
                    evalCount: datas[i].eval
                }
            ) %>
        <% } %>
    </table>
    <script src="staticJS/TopBarLabel.js"></script>
</body>
</html>